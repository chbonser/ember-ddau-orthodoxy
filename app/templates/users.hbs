{{#each model as |user|}}
  {{#paper-card as |card|}}
    {{#card.title as |title|}}
      {{#title.text as |text|}}
        {{#text.headline}}{{user.name}}{{/text.headline}}
      {{/title.text}}
    {{/card.title}}
    {{#card.actions}}
      {{#paper-button onClick=(action 'cardClick' user)}}Profile{{/paper-button}}
    {{/card.actions}}
  {{/paper-card}}
{{/each}}
